digraph tick {
	rankdir="LR";
	n_0 [label="tick", shape="plaintext"];
	n_1 [id="5", shape=circle, label="5"];
	n_2 [id="1", shape=circle, label="1"];
	n_0 -> n_2 [arrowhead=none];
	n_3 [id="2", shape=circle, label="2"];
	n_4 [id="4", shape=doublecircle, label="4"];
	n_5 [id="3", shape=circle, label="3"];

	n_3 -> n_5 [id="[$e|1]", label="spawn tickl"];
	n_5 -> n_1 [id="[$e|3]", label="spawn random"];
	n_1 -> n_4 [id="[$e|0]", label="send tack to tickl"];
	n_2 -> n_3 [id="[$e|2]", label="spawn tackl"];
}
